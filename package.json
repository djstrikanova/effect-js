{
  "name": "@effectai/effect-js",
  "version": "0.0.29",
  "description": "Effect Network Javscript/Typescript SDK (for [https://effect.network](https://effect.network))",
  "main": "dist/lib/index.js",
  "module": "dist/libs/index.js",
  "browser": "dist/bundle.umd.js",
  "unpkg": "dist/bundle.umd.js",
  "types": "dist/libs/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "echo \"ðŸ”¥\"",
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "bundle": "webpack",
    "rollup": "rollup -c rollup.config.js",
    "lint": "eslint \"src/**\"",
    "prepare": "husky install",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "prebuild": "rimraf docs dist",
    "build": "npm run build:cjs",
    "build:cjs": "tsc --module commonjs",
    "build:es2020": "tsc --module es2020",
    "build:umd": "tsc --module umd",
    "build:amd": "tsc --module amd",
    "build:system": "tsc --module system",
    "publish:latest": "npm version patch && npm run build && npm publish",
    "publish:next": "npm run prebuild && npm version patch &&  npm run build && npm publish --tag next ",
    "publish:cjs": "npm run prebuild && npm version patch &&  npm run build:cjs && npm publish --tag cjs ",
    "publish:umd": "npm run prebuild && npm version patch &&  npm run build:umd && npm publish --tag umd ",
    "publish:es2020": "npm run prebuild && npm version patch &&  npm run build:es && npm publish --tag es "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/effectai/effect-js.git"
  },
  "keywords": [
    "effect",
    "efx",
    "future",
    "work",
    "ai",
    "crypto",
    "blockchain",
    "microtask",
    "task",
    "micro-task"
  ],
